{
  "title": "DuRi â€¢ Phase Latency",
  "uid": "duri-phase-lat",
  "timezone": "browser",
  "links": [
    {
      "title": "Open Prometheus Targets",
      "type": "link",
      "url": "http://localhost:9090/targets",
      "targetBlank": true
    },
    {
      "title": "Share this dashboard (with vars)",
      "type": "link",
      "url": "/d/${__dashboard.uid}?orgId=1&var-phase=$phase&var-canary_tags=$canary_tags",
      "targetBlank": false
    }
  ],
  "templating": {
    "list": [
      {
        "name": "phase",
        "type": "query",
        "hide": 0,
        "label": "Phase",
        "query": "label_values(latency_seconds_bucket, phase)",
        "datasource": {
          "type": "prometheus",
          "uid": "PROM_DS"
        },
        "refresh": 1,
        "multi": true,
        "includeAll": true,
        "allValue": ".*",
        "current": { "selected": true, "text": ["All"], "value": ["$__all"] }
      },
      {
        "name": "canary_tags",
        "type": "custom",
        "hide": 0,
        "label": "Annotation tags",
        "query": "canary,duri",
        "multi": true,
        "includeAll": true,
        "allValue": "",
        "current": { "selected": true, "text": ["canary"], "value": ["canary"] }
      }
    ]
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "grafana" },
        "enable": true,
        "hide": false,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Canary",
        "tags": ["$canary_tags"],
        "type": "dashboard"
      }
    ]
  },
  "schemaVersion": 39,
  "refresh": "5s",
  "panels": [
    {
      "type": "timeseries",
      "title": "Phase p95 latency",
      "gridPos": {"x": 0, "y": 0, "w": 24, "h": 9},
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 1
        }
      },
      "targets": [
        {
          "expr": "duri:latency_p95{phase=~\"$phase\"}",
          "legendFormat": "{{phase}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Phase p99 latency",
      "gridPos": {"x": 0, "y": 9, "w": 24, "h": 9},
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 1
        }
      },
      "targets": [
        {
          "expr": "duri:latency_p99{phase=~\"$phase\"}",
          "legendFormat": "{{phase}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Phase avg latency (1m)",
      "gridPos": {"x": 0, "y": 18, "w": 24, "h": 9},
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 1
        }
      },
      "targets": [
        {
          "expr": "duri:latency_avg{phase=~\"$phase\"}",
          "legendFormat": "{{phase}}",
          "refId": "A"
        }
      ]
    }
  ],
  "version": 1
}
